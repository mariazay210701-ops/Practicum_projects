   "source": [
    "# добавляем тарифные параметры\n",
    "tariffs = (\n",
    "    data.groupby(['user_id', 'month'], as_index=False)\n",
    "        .agg(\n",
    "            start_ride_price = ('start_ride_price','last'),\n",
    "            minute_price     = ('minute_price','last'),\n",
    "            subscription_fee = ('subscription_fee','last'),\n",
    "            subscription_type= ('subscription_type','last')   # ← тут\n",
    "        )\n",
    ")\n",
    "\n",
    "# объединяем датафреймы\n",
    "agg_data = agg_data.merge(tariffs, on=['user_id','month'], how='left')\n",
    "\n",
    "# считаем выручку по формуле\n",
    "agg_data['month_revenue'] = (\n",
    "    agg_data['start_ride_price'] * agg_data['rides_count']\n",
    "    + agg_data['minute_price'] * agg_data['total_duration']\n",
    "    + agg_data['subscription_fee']\n",
    ")\n",
    "\n",
    "# оставляем только нужные столбцы\n",
    "agg_data = agg_data[['user_id','month','subscription_type','total_distance','rides_count','total_duration','month_revenue']]\n",
    "\n",
    "display(agg_data.head())"
   ]
  },
